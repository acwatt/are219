@article{graddy-lovelaceSupplyManagementAgricultural2017,
  title = {From Supply Management to Agricultural Subsidies\textemdash and Back Again? {{The U}}.{{S}}. {{Farm Bill}} \& Agrarian (in)Viability},
  shorttitle = {From Supply Management to Agricultural Subsidies\textemdash and Back Again?},
  author = {{Graddy-Lovelace}, Garrett and Diamond, Adam},
  year = {2017},

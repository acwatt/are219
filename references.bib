  institution = {{Social Science Research Network}},
  doi = {10.2139/ssrn.3610422},
  abstract = {Many US states and local areas have implemented stay-at-home and business closure policies to reduce the spread of coronavirus. We estimate the effects of these policies on social distancing and economic outcomes using event study designs, and we estimate the effects on disease transmission using a regression framework derived from an epidemiological model. We find that stay-at-home orders reduce the rate of disease transmission by 5\textendash 10 percent, and also reduce measures of economic activity by about the same amount in the short-run. A decomposition exercise suggests that the ratio of health benefits to economic impacts is similar for the social distancing induced by policy and the largely voluntary social distancing that preceded it.},
  langid = {english},
  keywords = {Coronavirus,Disease transmission,Health behaviors,Public policy},

@article{vatnChoicesPricesApologies1994,
  title = {Choices without {{Prices}} without {{Apologies}}},
  author = {Vatn, Arild and Bromley, Daniel W.},
  year = {1994},
  month = mar,
  journal = {Journal of Environmental Economics and Management},
  volume = {26},

@article{linObservationPM2Using2020,
  title = {Observation of {{PM2}}.5 Using a Combination of Satellite Remote Sensing and Low-Cost Sensor Network in {{Siberian}} Urban Areas with Limited Reference Monitoring},
  author = {Lin, Changqing and Labzovskii, Lev D. and Leung Mak, Hugo Wai and Fung, Jimmy C. H. and Lau, Alexis K. H. and Kenea, Samuel Takele and Bilal, Muhhamad and Vande Hey, Joshua D. and Lu, Xingcheng and Ma, Jun},
  year = {2020},
